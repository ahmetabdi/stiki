<div class="row-fluid">
  <div class="span8">
    <div class="row-fluid">
      <ul class="nav nav-list">
        <%- @pages.each do |page| %>
          <li>
            <a href="#"><%= page.title %></a>
          </li>
        <% end -%>
      </ul>
    </div>
  </div>
</div>
<div class="row-fluid">
  <div class="span8">
    <div class="row-fluid">
      <%= form_tag( stiki.spaces_path, :html => {:class => 'form-horizontal'} ) do |f| %>
        <fieldset class="well">
          <div class="control-group">
              <%= label_tag 'name', 'Name', :class => "control-label" %>
              <div class="controls">
                <input class="input-xlarge" id="name" name="space[name]" size="30" type="text">
              </div>
            <div class="form-actions">
              <%= submit_tag "Create", :class => "btn btn-primary" %>
            </div>
        </fieldset>
        <% end -%>
    </div>
  </div>
</div>